<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @post.body %>
</p>

<p>
  <strong>Photo:</strong>
  <%= attachment_image_tag @post, :photo, :fill, 100, 100, format: "jpg" %>
</p>
<p>
<strong>タグ:</strong>
<%#= render 'posts/tag_list' @post, tag_list: post.interest_list %>
</p>

<% if current_user.id == @post.user_id %>

<%= link_to '編集', edit_post_path(@post) %> |
<%= link_to '削除', @post, method: :delete, data: { confirm: '本当に削除しますか?' } %> |
<% end %>
<%= link_to '戻る', posts_path %>
